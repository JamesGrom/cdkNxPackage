{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cdk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-aws-cdk",
        "tsConfig": "packages/nx-aws-cdk/tsconfig.lib.json",
        "packageJson": "packages/nx-aws-cdk/package.json",
        "main": "packages/nx-aws-cdk/src/index.ts",
        "assets": [
          "packages/nx-aws-cdk/*.md",
          {
            "input": "./packages/nx-aws-cdk/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-aws-cdk/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-aws-cdk",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-aws-cdk",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cdk/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/cdk"],
      "options": {
        "jestConfig": "packages/cdk/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
